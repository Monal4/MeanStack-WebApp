$('#nav-bar-toggler').on('click',function(e){
  $('.collapse').on('show.bs.collapse',function(){
   $('#navbarNavDropdown').css('background-color','white')
   $('#navbarNavDropdown').css('z-index','999')
   $('main').css('z-index','-1')
   $('main').css('filter','blur(2px)')
  });
  $('.collapse').on('hidden.bs.collapse',function(){$('#navbarNavDropdown').css('background-color','white')});
  $('main').css('z-index','0')
  $('main').css('filter','blur(0)')
})



})
</script>
  
